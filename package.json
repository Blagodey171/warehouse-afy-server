{
    "name": "warehouse-webpack",
    "version": "1.0.0",
    "description": "warehouse for AFY",
    "main": "./index.js",
    "scripts": {
        "dev": "nodemon ./index.ts cross-env NODE_ENV=development",
        "build-server-tsc": "tsc ./index.ts",
        "build": "cross-env NODE_ENV=production webpack",
        "build-dev": "cross-env NODE_ENV=development webpack",
        "pm2-dev": "concurrently \"tsc --watch ./index.ts\" \"pm2 start ./index.js --exp-backoff-restart-delay=1000 --watch --wait-ready\"",
        "pm2-dev:delete": "pm2 delete ./index.js",
        "pm2-prod": "pm2 start dist/server.node.js --exp-backoff-restart-delay=1000 --watch --wait-ready",
        "pm2-prod:delete": "pm2 delete dist/server.node.js",
        "prod-test-start": "node dist/server.node.js"
    },
    "repository": {
        "type": "git",
        "url": "Blagodey171"
    },
    "author": "Denis P.",
    "license": "ISC",
    "dependencies": {
        "@babel/polyfill": "^7.12.1",
        "@types/webpack": "^5.28.0",
        "bcryptjs": "^2.4.3",
        "body-parser": "^1.19.0",
        "concurrently": "^6.1.0",
        "connect-mongo": "^4.5.0",
        "cookie-parser": "^1.4.5",
        "cors": "^2.8.5",
        "dotenv": "^10.0.0",
        "express": "^4.17.1",
        "express-session": "^1.17.2",
        "express-validator": "^6.11.1",
        "jsonwebtoken": "^8.5.1",
        "mongodb": "^4.1.3",
        "mongodb-client-encryption": "^1.2.7",
        "mongoose": "^5.12.10",
        "reselect": "^4.0.0"
    },
    "devDependencies": {
        "@babel/core": "^7.14.3",
        "@babel/preset-env": "^7.14.2",
        "@babel/preset-react": "^7.13.13",
        "@babel/preset-typescript": "^7.13.0",
        "@types/bcryptjs": "^2.4.2",
        "@types/express": "^4.17.13",
        "@types/jsonwebtoken": "^8.5.5",
        "@types/node": "^16.10.3",
        "babel-loader": "^8.2.2",
        "cross-env": "^7.0.3",
        "nodemon": "^2.0.13",
        "ts-loader": "^9.2.3",
        "ts-node": "^10.3.0",
        "typescript": "^4.3.2",
        "uglifyjs-webpack-plugin": "^2.2.0",
        "webpack": "^5.37.1",
        "webpack-bundle-analyzer": "^4.5.0",
        "webpack-cli": "^4.7.0",
        "webpack-dev-server": "^3.11.2"
    },
    "browserslist": "> 0.25%, not dead"
}
